<Window x:Class="Технологии.NewPartner"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Технологии"
        mc:Ignorable="d"
        Title="NewPartner" Height="450" Width="800">
    <Grid>
        <Rectangle HorizontalAlignment="Left" Height="340" Margin="179,64,0,0" Stroke="Black" VerticalAlignment="Top" Width="585" Fill="#FFBBDCFA"/>
        <Image HorizontalAlignment="Left" Height="100" Margin="10,10,0,0" VerticalAlignment="Top" Width="100" Source="/Новые технологии.png"/>
        <Button Content="Добавить партнёра" HorizontalAlignment="Left" Margin="10,195,0,0" VerticalAlignment="Top" Height="34" Width="147" Background="#FF0C4882" Foreground="White" Click="Button_Click"/>

        <Label Content="Новые технологии: новый заказ" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" Height="36" Width="263" FontSize="20" FontFamily="Bahnschrift Light SemiCondensed"/>
        <Label Content="Название партнёра" HorizontalAlignment="Left" Margin="205,84,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="name" HorizontalAlignment="Left" Margin="205,115,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="153" Height="28"/>
        <Label Content="Тип партнёра" HorizontalAlignment="Left" Margin="205,153,0,0" VerticalAlignment="Top"/>
        <Label Content="Директор" HorizontalAlignment="Left" Margin="205,217,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="dir" HorizontalAlignment="Left" Margin="205,248,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="153" Height="28"/>
        <Label Content="Почта" HorizontalAlignment="Left" Margin="205,276,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="email" HorizontalAlignment="Left" Margin="205,307,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="153" Height="28"/>
        <Label Content="Номер телефона" HorizontalAlignment="Left" Margin="453,84,0,0" VerticalAlignment="Top"/>
        <TextBox MaxLength="12" x:Name="phon" HorizontalAlignment="Left" Margin="453,115,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="153" Height="28" PreviewTextInput="phon_PreviewTextInput"/>
        <Label Content="Юридический адрес" HorizontalAlignment="Left" Margin="453,153,0,0" VerticalAlignment="Top"/>
        <TextBox  x:Name="uradres" HorizontalAlignment="Left" Margin="453,184,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="153" Height="28"/>
        <Label Content="ИНН"  HorizontalAlignment="Left" Margin="453,217,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="inn" MaxLength="10" HorizontalAlignment="Left" Margin="453,248,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="153" Height="28" PreviewTextInput="inn_PreviewTextInput"/>
        <Label Content="Рейтинг" HorizontalAlignment="Left" Margin="453,276,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="rate" MaxLength="2" HorizontalAlignment="Left" Margin="453,307,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="153" Height="28" PreviewTextInput="rate_PreviewTextInput"/>
        <Button Content="Назад" HorizontalAlignment="Left" Margin="10,245,0,0" VerticalAlignment="Top" Height="34" Width="147" Background="#FF0C4882" Foreground="White" Click="Button_Click_1"/>
        <ComboBox x:Name="parttype" DisplayMemberPath="Partner_type" HorizontalAlignment="Left" Margin="205,184,0,0" VerticalAlignment="Top" Width="153" Height="28"/>


    </Grid>
</Window>
